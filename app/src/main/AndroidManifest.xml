<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="android.baodian.com.biaodiangithub">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name=".MainApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:icon,android:theme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="android.baodian.com.biaodiangithub.login.LoginActivity" />
        <activity android:name="android.baodian.com.biaodiangithub.login.RegisterActivity" />
        <activity
            android:name="android.baodian.com.biaodiangithub.tab1.TaskInfoListActivity"
            android:label="@string/title_activity_task_info_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="android.baodian.com.biaodiangithub.tab1.TaskSubmitActivity" />
        <activity android:name="android.baodian.com.biaodiangithub.baesclass.BaseActivity"></activity>
    </application>

</manifest>